{
  "name": "parcell-key-manager",
  "version": "0.0.0",
  "scripts": {
    "start": "ng serve --configuration web --poll 1000",
    "build": "ng build && npm run capacitor:sync",
    "electron:run": "npm run build && electron ./electron/dev.js",
    "electron:build": "ng build --configuration windows --base-href './' && cd electron && npx electron-builder -c ./electron-builder.json",
    "capacitor:build": "ng build --configuration android && cap sync && cap open android"
  },
  "private": true,
  "dependencies": {
    "@angular/cdk": "^19.2.2",
    "@angular/common": "^19.2.0",
    "@angular/compiler": "^19.2.0",
    "@angular/core": "^19.2.0",
    "@angular/forms": "^19.2.0",
    "@angular/material": "^19.2.2",
    "@angular/platform-browser": "^19.2.0",
    "@angular/platform-browser-dynamic": "^19.2.0",
    "@angular/router": "^19.2.0",
    "@capacitor-community/sqlite": "^7.0.0",
    "@capacitor/android": "^7.0.1",
    "@capacitor/angular": "^2.0.3",
    "@capacitor/core": "^7.0.1",
    "@capacitor/keyboard": "^7.0.0",
    "@capacitor/toast": "^7.0.0",
    "@ng-bootstrap/ng-bootstrap": "^18.0.0",
    "@popperjs/core": "^2.11.8",
    "bootstrap": "^5.3.3",
    "capacitor-plugin-safe-area": "^4.0.0",
    "crypto-js": "^4.2.0",
    "dexie": "^4.0.11",
    "ngx-electron": "^2.2.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "ulid": "^2.4.0",
    "validator": "^13.12.0",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^19.2.1",
    "@angular/cli": "^19.2.1",
    "@angular/compiler-cli": "^19.2.0",
    "@angular/localize": "^19.2.0",
    "@capacitor/cli": "latest",
    "@faker-js/faker": "^9.6.0",
    "@types/jasmine": "~5.1.0",
    "@types/validator": "^13.12.2",
    "eslint-plugin-prettier": "^5.2.3",
    "jasmine-core": "~5.6.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
